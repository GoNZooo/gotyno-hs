struct SomeType {
    type: "SomeType"
    some_field: String
    some_other_field: I32
    maybe_some_field: ?String
}

struct Holder <T>{
    value: T
}

union Event {
    Notification: String
    Launch
    AnotherEvent: SomeType
}

union(tag = kind) EventWithKind {
    NotificationWithKind: String
    LaunchWithKind
    AnotherEventWithKind: SomeType
}

union Possibly <T>{
    NotReally
    Definitely: T
}